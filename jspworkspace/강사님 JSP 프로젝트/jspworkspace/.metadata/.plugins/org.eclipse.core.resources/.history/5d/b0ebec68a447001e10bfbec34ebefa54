	Cookie country = new Cookie("Shipping_", URLEncoder.encode(request.getParameter(""), "utf-8"));
	Cookie zipCode = new Cookie("Shipping_", URLEncoder.encode(request.getParameter(""), "utf-8"));
	Cookie addressName = new Cookie("Shipping_", URLEncoder.encode(request.getParameter(""), "utf-8"));
%>
